(
    version: "1",
    name: "vintage_story",
    author: "popcar2",
    description: Some("Vintage Story wiki"),
    is_enabled: true,
    domains: [],
    urls: [
        "https://wiki.vintagestory.at/"
    ],
    rules: [
        // Exclude different languages
        SkipURL("https://wiki.vintagestory.at/*/en"),
        SkipURL("https://wiki.vintagestory.at/*/de"),
        SkipURL("https://wiki.vintagestory.at/*/es"),
        SkipURL("https://wiki.vintagestory.at/*/pl"),
        SkipURL("https://wiki.vintagestory.at/*/ru"),
        SkipURL("https://wiki.vintagestory.at/*/fr"),
        SkipURL("https://wiki.vintagestory.at/*/it"),
        SkipURL("https://wiki.vintagestory.at/*/pt-br"),
        SkipURL("https://wiki.vintagestory.at/*/uk"),
        SkipURL("https://wiki.vintagestory.at/*/ja"),
        // Ignore misc wiki pages
        SkipURL("https://wiki.vintagestory.at/*&action=*"),
        SkipURL("https://wiki.vintagestory.at/*&oldid=*"),
        SkipURL("https://wiki.vintagestory.at/*Talk:*"),
        SkipURL("https://wiki.vintagestory.at/*printable=*"),
